<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="security.acl.voter.admin_permissions" class="SUS\AdminBundle\Voter\AdminAclVoter" public="false">
            <tag name="monolog.logger" channel="security" />
            <argument type="service" id="security.acl.provider" />
            <argument type="service" id="security.acl.object_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.security_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.permission.map" />
            <argument type="service" id="logger" on-invalid="null" />
            <tag name="security.voter" priority="255" />
        </service>
        <service id="security.acl.voter.phone_circuit_permissions" class="SUS\AdminBundle\Voter\PhoneCircuitAclVoter" public="false">
            <tag name="monolog.logger" channel="security" />
            <argument type="service" id="security.acl.provider" />
            <argument type="service" id="security.acl.object_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.security_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.permission.map" />
            <argument type="service" id="sonata.admin.pool" />
            <argument type="service" id="logger" on-invalid="null" />
            <tag name="security.voter" priority="254" />
        </service>
        <service id="security.acl.voter.request_permissions" class="SUS\AdminBundle\Voter\RequestAclVoter" public="false">
            <tag name="monolog.logger" channel="security" />
            <argument type="service" id="security.acl.provider" />
            <argument type="service" id="security.acl.object_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.security_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.permission.map" />
            <argument type="service" id="logger" on-invalid="null" />
            <tag name="security.voter" priority="253" />
        </service>
        <service id="sonata.admin.dnna.model_manager" class="SUS\AdminBundle\Model\DnnaModelManager">
            <argument type="service" id="doctrine" />
        </service>
        <service id="sonata.admin.dnna.datagridbuilder" class="SUS\AdminBundle\Builder\DnnaDatagridBuilder">
            <argument type="service" id="form.factory" />
            <argument type="service" id="sonata.admin.builder.filter.factory" />
            <argument type="service" id="sonata.admin.guesser.orm_datagrid_chain" />
        </service>
        <!-- PhoneCircuit -->
        <service id="sonata.admin.phonecircuits.helpdesk" class="SUS\AdminBundle\Admin\Helpdesk\PhoneCircuitAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Τηλεπικοινωνιακά Κυκλώματα"
                label="Τηλεπικοινωνιακά Κυκλώματα"
                />
           <argument />
           <argument>SUS\SiteBundle\Entity\Circuits\PhoneCircuit</argument>
           <argument>SonataAdminBundle:CRUD</argument>
            <call method="setSecurityContext">
                <argument type="service" id="security.context" />
            </call>
            <!--<call method="setRequest">
                <argument type="service" id="request" />
            </call>-->
       </service>
        <service id="sonata.admin.phonecircuits.kedo" class="SUS\AdminBundle\Admin\Kedo\PhoneCircuitAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Τηλεπικοινωνιακά Κυκλώματα"
                label="Τηλεπικοινωνιακά Κυκλώματα"
                />
           <argument />
           <argument>SUS\SiteBundle\Entity\Circuits\PhoneCircuit</argument>
           <argument>SonataAdminBundle:CRUD</argument>
            <!--<call method="setRequest">
                <argument type="service" id="request" />
            </call>-->
       </service>
        <!-- NewCircuitRequest -->
        <service id="sonata.admin.newcircuitrequests.helpdesk" class="SUS\AdminBundle\Admin\Helpdesk\NewCircuitRequestAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
