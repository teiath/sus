                />
           <argument />
           <argument>SUS\SiteBundle\Entity\Requests\RemoveCircuitRequest</argument>
           <argument>SUSAdminBundle:Request</argument>
       </service>
       <!-- ChangeConnectivityTypeRequest -->
        <service id="sonata.admin.changeconnectivitytyperequests.helpdesk" class="SUS\AdminBundle\Admin\Helpdesk\ChangeConnectivityTypeRequestAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Αιτήματα"
                label="Αίτημα Αλλαγής Τύπου Κυκλώματος"
                />
           <argument />
           <argument>SUS\SiteBundle\Entity\Requests\ChangeConnectivityTypeRequest</argument>
           <argument>SUSAdminBundle:Request</argument>
       </service>
        <service id="sonata.admin.changeconnectivitytyperequests.kedo" class="SUS\AdminBundle\Admin\Kedo\ChangeConnectivityTypeRequestAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Αιτήματα"
                label="Αίτημα Αλλαγής Τύπου Κυκλώματος"
                />
           <argument />
           <argument>SUS\SiteBundle\Entity\Requests\ChangeConnectivityTypeRequest</argument>
           <argument>SUSAdminBundle:Request</argument>
       </service>
       <!-- ActivateServiceRequest -->
        <service id="sonata.admin.activateservicerequest.helpdesk" class="SUS\AdminBundle\Admin\Helpdesk\ActivateServiceRequestAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Αιτήματα"
                label="Αίτημα Ενεργοποίησης Υπηρεσίας σε Υπάρχον Ιδιωτικό Κύκλωμα"
                />
           <argument />
