       </service>
       <!-- ChangeOwnershipRequest -->
        <service id="sonata.admin.changeownershiprequest.helpdesk" class="SUS\AdminBundle\Admin\Helpdesk\ChangeOwnershipRequestAdmin">
           <tag name="sonata.admin"
                manager_type="orm"
                group="Αιτήματα"
                label="Αίτημα Αλλαγής Ιδιοκτησίας Κυκλώματος"
                />
           <argument />
           <argument>SUS\SiteBundle\Entity\Requests\ChangeOwnershipRequest</argument>
